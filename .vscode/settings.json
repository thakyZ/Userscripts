{
  "eslint.experimental.useFlatConfig": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.options": {
    "overrideConfigFile": "./eslint.config.mjs"
  },
  "eslint.debug": true,
  "files.associations": {
    "OnlineClock.Blocked.user.css": "stylus",
    "FFXIV_Lodestone_Maint_Dark.user.css": "stylus",
    "htmlhint.conf": "json",
    "*.json": "json",
    "invalidFileNameChars.json": "jsonc"
  },
  "editor.defaultFormatter": null,
  "editor.formatOnPaste": false, // required
  "editor.formatOnType": false, // required
  "editor.formatOnSave": false,
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sass]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "htmlhint.documentSelector": [
    "html",
    "htm",
    "!template.html"
  ],
  "[json]": {
    "editor.defaultFormatter": "j-brooke.fracturedjsonvsc"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "cSpell.words": [
    "an",
    "and",
    "autofixer",
    "camelcase",
    "churn",
    "Cooldown",
    "crafter",
    "disqus",
    "doesn't",
    "Dynamis",
    "elementor",
    "ESgreasemonkey",
    "favorited",
    "freelist",
    "gamerescape",
    "glamourdresser",
    "glyphicon",
    "GM_xmlhttpRequest",
    "greasemonkey",
    "has",
    "iframes",
    "Imgur",
    "is",
    "itemprop",
    "Keybinds",
    "linebreak-style",
    "nekogaming",
    "no-new-native-nonconstructor",
    "no-nonoctal-decimal-escape",
    "no-useless-backreference",
    "NOSONAR",
    "operator-linebreak",
    "rule",
    "smalloc",
    "stable",
    "testid",
    "the",
    "tidyup",
    "wrap-iife"
  ],
  "docwriter.style": "JSDoc",
  "[html]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  }
}
