{
  "eslint.useFlatConfig": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.options": {"overrideConfigFile": "./eslint.config.js"},
  "eslint.debug": true,
  "files.associations": {
    "OnlineClock.Blocked.user.css"       : "stylus",
    "FFXIV_Lodestone_Maint_Dark.user.css": "stylus",
    "htmlhint.conf"                      : "jsonc" ,
    "*.json"                             : "json"  ,
    "*.json5"                            : "jsonc" ,
    "tsconfig.json"                      : "jsonc" ,
    "tsconfig.*.json"                    : "jsonc" ,
    "cspell.json"                        : "jsonc" ,
  },
  "editor.defaultFormatter": null,
  "editor.formatOnPaste": false,  // required
  "editor.formatOnType": false,  // required
  "editor.formatOnSave": false,
  "[less]": {"editor.defaultFormatter": "dbaeumer.vscode-eslint"},
  "[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[css]": {"editor.defaultFormatter": "dbaeumer.vscode-eslint"},
  "[sass]": {"editor.defaultFormatter": "dbaeumer.vscode-eslint"},
  "[scss]": {"editor.defaultFormatter": "dbaeumer.vscode-eslint"},
  "[jsonc]": {"editor.defaultFormatter": "j-brooke.fracturedjsonvsc"},
  "[json]": {"editor.defaultFormatter": "j-brooke.fracturedjsonvsc"},
  "docwriter.style": "JSDoc",
  "htmlhint.documentSelector": ["html", "htm", "!template.html"],
  "javascript.inlayHints.enumMemberValues.enabled": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": false,
  "javascript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "javascript.inlayHints.parameterTypes.enabled": false,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": false,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "javascript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false
}
