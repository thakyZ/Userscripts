{
  "name": "thakyz-user-scripts",
  "version": "1.0.0",
  "description": "UserScripts by thakyZ.",
  "main": "index.js",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev:webpack": "webpack --config webpack/dev.ts",
    "build:webpack": "webpack --config webpack/prod.ts",
    "build:watch": "webpack --watch",
    "lessc": "npx ./node_modules/.bin/lessc",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint:eslint": "eslint -c eslint.config.js .",
    "lint": "npm run lint:eslint && npm run prettier:check",
    "lint:fix": "npm run lint:eslint && npm run prettier:write",
    "spellcheck": "npx cspell-cli --gitignore .",
    "build:make": "make",
    "build:doc": "toc-md README.md",
    "clean": "make clean",
    "rebuild": "make clean build",
    "typecheck": "make typecheck",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thakyZ/Userscripts.git"
  },
  "keywords": [
    "userscript",
    "user script",
    "userscripts",
    "user scripts",
    "css",
    "greasemonkey",
    "grease monkey",
    "tampermonkey",
    "tamper monkey",
    "stylish",
    "stylus",
    "typescript",
    "browser",
    "extension"
  ],
  "author": {
    "name": "Neko Boi Nick",
    "url": "https://github.com/thakyZ",
    "email": "950594+thakyZ@users.noreply.github.com"
  },
  "contributors": [
    {
      "name": "pboymt"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thakyZ/Userscripts/issues"
  },
  "homepage": "https://github.com/thakyZ/Userscripts#readme",
  "browserslist": [
    "Firefox ESR",
    "Last 2 Chrome versions"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./build": {
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./lib/*": {
      "import": "./lib/*.mjs",
      "require": "./lib/*.js"
    }
  },
  "files": [
    "build/*",
    "lib/*"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/greasemonkey": "^4.0.7",
    "@types/jest": "29.5.12",
    "@types/jquery": "^3.5.30",
    "@types/list.js": "^2.3.4",
    "@types/node": "^22.0.0",
    "@types/tampermonkey": "^5.0.3",
    "@types/unidecode": "^0.1.3",
    "@types/webpack": "^5.28.5",
    "cross-env": "^7.0.3",
    "cspell-cli": "^8.13.0",
    "data-uri-regex": "^0.1.4",
    "esbuild": "^0.23.0",
    "esbuild-plugin-userscript": "^0.2.6",
    "eslint": "^9.8.0",
    "eslint-config-greasemonkey": "^1.0.1",
    "eslint-config-jquery": "^3.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-stylelint": "^22.0.0",
    "eslint-plugin-jest": "file:../eslint-plugin-jest",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "glob": "^11.0.0",
    "globals": "^15.8.0",
    "gm-storage": "^2.0.3",
    "gzip-js": "^0.3.2",
    "htmlhint": "^1.1.4",
    "jest": "^29.7.0",
    "jquery": "^3.7.1",
    "less": "^4.2.0",
    "less-plugin-sass2less": "^1.2.0",
    "little-emitter": "^0.3.5",
    "nodejs-fs-utils": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "ts-jest": "29.2.3",
    "ts-node": "^10.9.2",
    "ts-preferences": "^2.0.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0-alpha.61",
    "userscript-metadata": "^1.1.0"
  },
  "dependencies": {
    "less-var-to-css-var": "^1.6.0",
    "strip-json-comments": "^5.0.1",
    "thakyz-user-scripts": "file:",
    "unidecode": "^1.1.0"
  }
}
